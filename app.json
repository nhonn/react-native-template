{
  "expo": {
    "name": "MyTemplateProject",
    "slug": "my-template-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mytemplateproject",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mytemplateproject",
      "buildNumber": "1",
      "infoPlist": {}
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.mytemplateproject",
      "versionCode": 1,
      "version": "1.0.0"
    },
    "plugins": ["expo-router", ["expo-splash-screen"]],
    "experiments": {
      "typedRoutes": true
    }
  }
}
